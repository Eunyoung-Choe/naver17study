package day1219;

class Orange{
	private String name;
	private String color;
	private int age;
	
	public void writeMember()
	{
		System.out.println("name="+name+", color="+color+", age="+age);
	}
}

public class Ex4Constructor {

	public static void main(String[] args) {
		
		Orange a1 = new Orange();
		a1.writeMember();
	}

}
