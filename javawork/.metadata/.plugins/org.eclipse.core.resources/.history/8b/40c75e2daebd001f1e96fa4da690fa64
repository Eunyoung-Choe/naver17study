package day1219;

public class Ex2ObjectArray {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int inwon;
		Student []stuArray;
		
		System.out.println("입력할 인원 수는?");
		inwon = Integer.parseInt(sc.nextLine());
		
		// 메모리 할당
		stuArray = new Student[inwon];
		
		
	}

}
